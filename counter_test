#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1b86f90 .scope module, "counter_tb" "counter_tb" 2 1;
 .timescale 0 0;
v0x1b95810_0 .var "iclk", 0 0;
v0x1b958e0_0 .var "irst", 0 0;
v0x1b95990_0 .net "ocnt", 3 0, v0x1b95770_0; 1 drivers
S_0x1b87080 .scope module, "counter_test" "counter" 2 13, 3 1, S_0x1b86f90;
 .timescale 0 0;
v0x1b50690_0 .net "iclk", 0 0, v0x1b95810_0; 1 drivers
v0x1b956d0_0 .net "irst", 0 0, v0x1b958e0_0; 1 drivers
v0x1b95770_0 .var "ocnt", 3 0;
E_0x1b630c0 .event posedge, v0x1b50690_0;
    .scope S_0x1b87080;
T_0 ;
    %wait E_0x1b630c0;
    %load/v 8, v0x1b956d0_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 4, 0;
    %assign/v0 v0x1b95770_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %ix/load 0, 1, 0;
    %load/vp0 8, v0x1b95770_0, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0x1b95770_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1b86f90;
T_1 ;
    %set/v v0x1b958e0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x1b86f90;
T_2 ;
    %set/v v0x1b95810_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x1b86f90;
T_3 ;
    %set/v v0x1b958e0_0, 1, 1;
    %delay 100, 0;
    %set/v v0x1b958e0_0, 0, 1;
    %delay 1900, 0;
    %vpi_call 2 8 "$stop";
    %delay 2000, 0;
    %vpi_call 2 10 "$finish";
    %end;
    .thread T_3;
    .scope S_0x1b86f90;
T_4 ;
    %delay 10, 0;
    %load/v 8, v0x1b95810_0, 1;
    %inv 8, 1;
    %set/v v0x1b95810_0, 8, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x1b86f90;
T_5 ;
    %vpi_call 2 19 "$monitor", "At time %t, ocnt = %d", $time, v0x1b95990_0;
    %end;
    .thread T_5;
    .scope S_0x1b86f90;
T_6 ;
    %vpi_call 2 22 "$dumpfile", "counter_test.vcd";
    %vpi_call 2 23 "$dumpvars", 1'sb0, S_0x1b87080;
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_tb.v";
    "counter.v";
